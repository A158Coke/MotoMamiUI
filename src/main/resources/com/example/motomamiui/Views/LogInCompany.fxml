<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="339.0" prefWidth="382.0" style="-fx-background-color: mistyrose;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.motomamiui.Controllers.LogInCompanyController">
    <HBox alignment="TOP_LEFT" layoutX="14.0" layoutY="14.0">
        <Button text="Back" onAction="#handleBack" />
    </HBox>
    <VBox alignment="CENTER" layoutX="14.0" layoutY="14.0" minHeight="298.0" minWidth="300" prefHeight="298.0" prefWidth="352.0" spacing="20">
        <Text style="-fx-font-size: 20pt;" text="LogInCompany" />
        <Label text="Email" />
        <TextField fx:id="emailField" maxWidth="200" promptText="Enter your email" />
        <Label text="Password" />
        <PasswordField id="PassInput" fx:id="passwordField" maxWidth="200" promptText="Enter your password" />
        <HBox alignment="CENTER" spacing="10">
            <Button fx:id="loginButton" text="Login" onAction="#handleLogin" />
        </HBox>
    </VBox>
</AnchorPane>
