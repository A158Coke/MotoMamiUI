<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" hgap="10" prefWidth="511.0" style="-fx-background-color: mistyrose; -fx-padding: 20;" vgap="10" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.motomamiui.Controllers.RegistrationController">
    <!-- 返回按钮 -->
    <HBox alignment="TOP_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <Button fx:id="backButton" onAction="#backToLogIn" text="Back" />
    </HBox>
    <Text style="-fx-font-size: 24pt;" text="User Registration" GridPane.columnIndex="1" GridPane.columnSpan="2" />

    <Label text="Name" GridPane.rowIndex="1" />
    <TextField fx:id="nameField" maxWidth="300" promptText="Enter your name" GridPane.columnIndex="1" GridPane.rowIndex="1" />

    <Label text="Surname" GridPane.rowIndex="2" />
    <TextField fx:id="surnameField" maxWidth="300" promptText="Enter your surname" GridPane.columnIndex="1" GridPane.rowIndex="2" />

    <Label text="Date of Birth" GridPane.rowIndex="3" />
    <DatePicker fx:id="dateOfBirthPicker" editable="false" maxWidth="300" promptText="Select date of birth" GridPane.columnIndex="1" GridPane.rowIndex="3" />

    <Label text="Address" GridPane.rowIndex="4" />
    <TextField fx:id="addressField" maxWidth="300" promptText="Enter your address" GridPane.columnIndex="1" GridPane.rowIndex="4" />

    <Label text="Phone Number" GridPane.rowIndex="5" />
    <TextField fx:id="phoneNumberField" maxWidth="300" promptText="Enter your phone number" GridPane.columnIndex="1" GridPane.rowIndex="5" />

    <Label text="ID Number or DNI" GridPane.rowIndex="6" />
    <TextField fx:id="licenseOrIDField" maxWidth="300" promptText="Enter your ID number or DNI" GridPane.columnIndex="1" GridPane.rowIndex="6" />

    <Label text="Driving License Number" GridPane.rowIndex="7" />
    <TextField fx:id="drivingLicenseNumberField" maxWidth="300" promptText="Enter your driving license number" GridPane.columnIndex="1" GridPane.rowIndex="7" />

    <Label text="Email" GridPane.rowIndex="8" />
    <TextField fx:id="emailField" maxWidth="300" promptText="Enter your email" GridPane.columnIndex="1" GridPane.rowIndex="8" />

    <Label text="Vehicle Type" GridPane.rowIndex="9" />
    <ComboBox fx:id="vehicleTypeComboBox" maxWidth="300" promptText="Select vehicle type" GridPane.columnIndex="1" GridPane.rowIndex="9">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Coche" />
                <String fx:value="Moto" />
                <String fx:value="Camion" />
                <String fx:value="Bici" />
                <String fx:value="patinete" />
            </FXCollections>
        </items>
    </ComboBox>

    <Label text="Reg. Number (if applicable)" GridPane.rowIndex="10" />
    <TextField fx:id="licensePlateField" maxWidth="300" prefHeight="26.0" prefWidth="262.0" promptText="Enter your vehicle registration number" GridPane.columnIndex="1" GridPane.rowIndex="10" />

    <Label text="Vehicle Brand (if applicable)" GridPane.rowIndex="11" />
    <TextField fx:id="vehicleBrandField" maxWidth="300" promptText="Enter the brand of your vehicle" GridPane.columnIndex="1" GridPane.rowIndex="11" />

    <Label text="Vehicle Model (if applicable)" GridPane.rowIndex="12" />
    <TextField fx:id="vehicleModelField" maxWidth="300" promptText="Enter your vehicle model" GridPane.columnIndex="1" GridPane.rowIndex="12" />

    <Label text="Password" GridPane.rowIndex="13" />
    <PasswordField fx:id="passwordField" maxWidth="300" promptText="Enter your password" GridPane.columnIndex="1" GridPane.rowIndex="13" />

    <Label text="Gender" GridPane.rowIndex="14" />
    <ComboBox fx:id="genderComboBox" maxWidth="300" promptText="Select gender" GridPane.columnIndex="1" GridPane.rowIndex="14">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Hombre" />
                <String fx:value="Mujer" />
            </FXCollections>
        </items>
    </ComboBox>

    <Button minWidth="200" onAction="#registro" prefHeight="34.0" prefWidth="374.0" text="Register" GridPane.columnSpan="2" GridPane.rowIndex="15" />

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0" />
      <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
        <RowConstraints />
    </rowConstraints>
</GridPane>
